<!doctype html>
<html>

<head>

    <link rel="icon" href="/img/iconMain-10.png">
    <link rel='stylesheet' href="/css/styles.css">

</head>

<body>

    {{>header}}

    <title> in-transit store </title>

    <div class="container main-content">
        <h1 id="title"> Visualize Your RedBubble Sales! </h1>
        <div class="post">
                <form action="store" method="post" enctype="multipart/form-data">
                    <h2> Select CSV to upload: </h2>
                    <input type="file" name="fileToUpload" id="fileToUpload">
                    <input type="submit" value="Upload CSV File" name="submit" id="submit">
                </form>
        </div>
    </div>

    {{>footer}}

</body>

</html>